{
  "name": "odata-v4-server-mysql-example",
  "version": "0.1.0",
  "description": "OData V4 Server with MySQL example",
  "main": "lib/index.js",
  "typings": "lib/index",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "mocha test/*.spec.js --reporter mochawesome --reporter-options reportDir=report,reportName=odata-v4-server,reportTitle=\"OData V4 Server\"",
    "pretest": "npm run build",
    "docker-build": "docker build -t odata-v4-server-mysql-example .",
    "docker-run": "docker run --name odata-v4-server-mysql-example --link mongo:mongo -p 3000:3000 -d odata-v4-server-mysql-example",
    "docker-stop": "docker stop odata-v4-server-mysql-example",
    "docker-clean-containers": "docker rm $(docker ps -a -q)",
    "docker-clean-images": "docker rmi $(docker images -a -q)",
    "docker-clean": "npm run docker-clean-containers && npm run docker-clean-images",
    "docker-enter": "docker exec -ti odata-v4-server-mysql-example bash"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaystack/odata-v4-server-mysql-example.git"
  },
  "author": "JayStack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaystack/odata-v4-server-mysql-example/issues"
  },
  "homepage": "https://github.com/jaystack/odata-v4-server-mysql-example#readme",
  "keywords": [
    "odata",
    "server",
    "mysql"
  ],
  "devDependencies": {
    "@types/mysql": "0.0.30",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "mochawesome": "^1.5.4",
    "typescript": "^2.0.10"
  },
  "dependencies": {
    "mysql": "^2.12.0",
    "odata-v4-mysql": "^0.1.0",
    "odata-v4-server": "rc"
  }
}
